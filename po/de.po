msgid "Dictionary Client"
msgstr "Wörterbuchprogramm"

#: ../lib/spell.c:83
#, c-format
msgid "%d suggestion found."
msgid_plural "%d suggestions found."
msgstr[0] "%d Vorschlag gefunden."
msgstr[1] "%d Vorschläge gefunden."

